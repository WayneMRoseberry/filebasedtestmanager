{
  "watch": [
    "src/",
    "config/",
    "schemas/"
  ],
  "ext": "js,json,yaml,yml",
  "ignore": [
    "node_modules/",
    "tests/",
    "coverage/",
    "dist/",
    "build/"
  ],
  "exec": "node src/server/app.js",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true
}
